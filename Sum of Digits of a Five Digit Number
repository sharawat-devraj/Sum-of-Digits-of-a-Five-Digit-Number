#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
	   int n,sum=0,x,l;
       scanf("%d",&n);
       x=n;
       while(x>0)
       {
           l=x%10;
           sum=sum+l;
           x=x/10;
       }
        
        printf("%d",sum);
    
    return 0;
}
